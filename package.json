{
  "name": "@ebay/jquery-next-id",
  "description": "jQuery collection plugin that will assign the next ID in sequence if an ID property does not already exist.",
  "version": "0.0.8",
  "scripts": {
    "start": "nodemon -e js -x 'npm test' --ignore coverage -s",
    "test": "npm run lint && karma start",
    "prepublish": "npm run minify",
    "lint": "jshint jquery.nextid.js > jshint.txt",
    "minify": "npm test && jsmin -o jquery.nextid.min.js jquery.nextid.js",
    "clean": "rimraf coverage target karma_html jshint.txt"
  },
  "author": "Ian McBurnie <ianmcburnie@hotmail.com>",
  "homepage": "https://github.com/ianmcburnie/jquery-next-id",
  "bugs": {
    "url": "https://github.com/ianmcburnie/jquery-next-id/issues"
  },
  "keywords": [
    "jquery-plugin",
    "ecosystem:jquery",
    "ebay"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ianmcburnie/jquery-next-id.git"
  },
  "license": "ISC",
  "devDependencies": {
    "coveralls": "^2.11.4",
    "jasmine-core": "^2.3.4",
    "jquery": "1.11.2",
    "jshint": "^2.8.0",
    "jsmin": "^1.0.1",
    "karma": "^0.13.9",
    "karma-coverage": "^0.5.0",
    "karma-html-reporter": "^0.2.6",
    "karma-jasmine": "^0.3.6",
    "karma-junit-reporter": "^0.3.3",
    "karma-phantomjs-launcher": "^0.2.1",
    "nodemon": "^1.8.1",
    "phantomjs": "^1.9.18",
    "rimraf": "^2.4.4"
  }
}
