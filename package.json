{
  "name": "jquery-next-id",
  "description": "jQuery collection plugin that will assign the next ID in sequence if an ID property does not already exist.",
  "version": "0.0.16",
  "scripts": {
    "start": "karma start --autoWatch true --singleRun false",
    "test": "karma start --autoWatch false --singleRun true",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build",
    "build": "npm run jsdoc && npm run minify",
    "clean": "rimraf jsdoc test_reports *.min.js lint.txt",
    "fix": "eslint jquery.nextid.js --fix",
    "jsdoc": "jsdoc jquery.nextid.js -d jsdoc",
    "lint": "eslint jquery.nextid.js > lint.txt",
    "minify": "jsmin -o jquery.nextid.min.js jquery.nextid.js"
  },
  "pre-commit": "prepublish",
  "author": "Ian McBurnie <ianmcburnie@hotmail.com>",
  "homepage": "https://github.com/ianmcburnie/jquery-next-id",
  "bugs": {
    "url": "https://github.com/ianmcburnie/jquery-next-id/issues"
  },
  "keywords": [
    "jquery-plugin",
    "ecosystem:jquery",
    "ebay",
    "accessibility",
    "a11y"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ianmcburnie/jquery-next-id.git"
  },
  "license": "ISC",
  "devDependencies": {
    "coveralls": "^2.11.9",
    "eslint": "^2.12.0",
    "eslint-config-google": "^0.4.0",
    "jasmine-core": "^2.4.1",
    "jsdoc": "^3.4.0",
    "jsmin": "^1.0.1",
    "jquery": ">=1.11",
    "karma": "^0.13.22",
    "karma-coverage": "^1.0.0",
    "karma-html-reporter": "^0.2.7",
    "karma-jasmine": "^1.0.2",
    "karma-junit-reporter": "^1.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "phantomjs-prebuilt": "^2.1.7",
    "pre-commit": "^1.1.3",
    "rimraf": "^2.5.2"
  },
  "peerDependencies": {
    "jquery": ">=1.11"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
